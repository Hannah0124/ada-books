<%# reference: https://github.com/Ada-Developers-Academy/textbook-curriculum/blob/master/09-intermediate-rails/session.md %>

<%= form_with model: @user, class: 'login__form', url: login_path do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.submit "Log In", class: "btn btn-primary" %>
<% end %>